@model TaxiBookingServiceUI.Models.DriverComplaintViewModel

@{
    ViewData["Title"] = "Complaint";
    <p style="color : red">@TempData["message"]</p>
}

<h1>Complaint</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Complaint">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Complaint" class="control-label"></label>
                <input asp-for="Complaint" class="form-control" />
                <span asp-validation-for="Complaint" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Submit" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



